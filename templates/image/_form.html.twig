<form action="{{ path('image_new',{'pid': pid}) }}" method="post" enctype="multipart/form-data">
    Select image to upload:
    <input name="productid" type="hidden" value="{{ pid }}" type="number" >
    <input type="file"  name="imagename" id="fileToUpload">
    <input type="submit" value="Upload Image" name="submit">
</form>
